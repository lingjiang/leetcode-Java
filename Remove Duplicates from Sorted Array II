public class Solution {
    public int removeDuplicates(int[] A) {
        if(A == null || A.length == 0)
            return 0;
            
        int len = 1;
        int pre = A[0];
        for(int i = 1; i < A.length; i++){
            if(A[i] != pre){
                A[len] = A[i];
                pre = A[i];
                len++;
            }
        }
        return len;
    }
}
